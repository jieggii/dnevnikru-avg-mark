{

  "manifest_version": 2,
  "name": "Dnevnik.ru average mark",
  "version": "1.0",

  "description": "Adds a new cell into marks table at schools.dnevnik.ru/marks.aspx - \"Average mark\"",
  "icons": {
    "48": "icons/five.png"
  },

  "applications": {
    "gecko": {
      "id": "dnevnik@mozilla.org",
      "strict_min_version": "45.0"
    }
  },

  "content_scripts": [
    {
      "matches": ["*://schools.dnevnik.ru/marks.aspx*"],
      "js": ["app.js"]
    }
  ]

}